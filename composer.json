{
    "name": "lettered/support",
    "description": "lettered support package",
    "keywords": ["goeasy", "opt", "socket", "auth"],
    "type": "library",
    "support": {
        "issues": "https://github.com/lettered/support/issues",
        "source": "https://github.com/lettered/support"
    },
    "authors": [
        {
            "name": "lettered",
            "email": "me@lettered.cn"
        }
    ],
    "require": {
        "php": ">=7.1.3",
